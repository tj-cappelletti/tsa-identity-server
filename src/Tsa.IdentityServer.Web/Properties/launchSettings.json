{
    "iisSettings": {
        "windowsAuthentication": false,
        "anonymousAuthentication": true,
        "iisExpress": {
            "applicationUrl": "http://localhost:62047",
            "sslPort": 44353
        }
    },
    "profiles": {
        "IIS Express": {
            "commandName": "IISExpress",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        "Tsa.IdentityServer.Web": {
            "commandName": "Project",
            "commandLineArgs": "--CREATE_DB true --DB_SEED_SOURCE project",
            "launchBrowser": true,
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "dotnetRunMessages": "true",
            "applicationUrl": "https://localhost:5001;http://localhost:5000"
        },
        "Docker": {
            "commandName": "Docker",
            "commandLineArgs": "--CREATE_DB true --DB_SEED_SOURCE system --DB_SEED_SOURCE_LOCATION /data/identityserverseeddata.json",
            "launchBrowser": true,
            "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}",
            "environmentVariables": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "publishAllPorts": true,
            "useSSL": true
        }
    }
}